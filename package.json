{
  "name": "honeyc",
  "displayName": "HoneyC",
  "description": "Base Language Support For HoneyC",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.97.0"
  },
  "publisher": "Isaac Honeyman",
  "author": "Isaac Honeyman",
